/* Fade in left */
.fadeInLeft{
   background-image: url(https://images.pexels.com/photos/4245826/pexels-photo-4245826.jpeg?cs=srgb&dl=pexels-riccardo-bertolo-4245826.jpg&fm=jpg);
  -webkit-animation: fadeInLeft 1.2s both; /* Safari 4+ */
  -moz-animation: fadeInLeft 1.2s both; /* Fx 5+ */
  -o-animation: fadeInLeft 1.2s both; /* Opera 12+ */
  animation: fadeInLeft 1.2s both; /* IE 10+, Fx 29+ */
}

/* Safari 4+ */
@-webkit-keyframes fadeInLeft {
  0% {
      opacity: 0;
      -webkit-transform: translateX(-50px);
  }
  100% {
      opacity: 1;
      -webkit-transform: translateX(0);
  }
}

/* Fx 5+ */
@-moz-keyframes fadeInLeft {
  0% {
      opacity: 0;
      -moz-transform: translateX(-50px);
  }
  100% {
      opacity: 1;
      -moz-transform: translateX(0);
  }
}

/* Opera 12+ */
@-o-keyframes fadeInLeft {
  0% {
      opacity: 0;
      -o-transform: translateX(-50px);
  }
  100% {
      opacity: 1;
      -o-transform: translateX(0);
  }
}

/* IE 10+, Fx 29+ */
